.woap {
  width: 96%;
  margin: 50rpx auto;
  font-size: 28rpx;
}
.lst {
  width: 100%;
  height: 110rpx;
  border-bottom: 1rpx solid #eee;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.lst input{
  width: 80%;
}
.lst image{
  width: 0rpx;
  height: 0rpx;
  margin-left: 10rpx;
}
.lst view{
  width: 60%;
  display: flex;
  align-items: center;
}
.tiile{
  width: 40%;
  margin-left: 20rpx;
}
