/* pages/info/estimate/estimate.wxss */
.woap{
  width: 90%;
  margin: 50rpx auto;
}
.item{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.left{
  width: 30%;
  height: 180rpx;
  border-radius: 20rpx;
  overflow: hidden;
}
.left image{
  width: 100%;
  height: 100%;
}
.right{
  width: 65%;
}
.title{
  width: 100%;
  height: 60rpx;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  color: #000;
  padding-left: 10rpx;
}
.name{
  font-size: 28rpx;
  color: #000;
  height: 50rpx;
  display: flex;
  padding-left: 10rpx;
}
.int{
  width: 100%;
  height: 330rpx;
  border-radius: 20rpx;
  border: 1rpx solid #979797;
  margin-top: 80rpx;
  position: relative;
}
.names{
  width: 92%;
  max-height: 150rpx;
  margin: 30rpx auto;
  font-size: 28rpx;
}
.img{
  width: 242rpx;
  height: 126rpx;
  border-radius: 20rpx;
  border:1rpx solid #979797;
  position: absolute;
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
}
.img text{
  font-size: 28rpx;
  color: #666;
}
.img image{
  width: 100%;
  height: 100%;
  object-fit: cover;
}
.btns{
  width: 90%;
  height: 84rpx;
  margin: 30rpx auto;
  margin-top: 160rpx;
  background: linear-gradient(360deg, #F23232 0%, #F21E1E 55%, #FF7979 100%);
  box-shadow: 0px 4px 8px 0px rgba(0,0,0,0.05);
  border-radius: 20rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: center;
}
.chang{
  width: 80%;
  display: flex;
  align-items: center;
  justify-items: center;
  flex-wrap: wrap;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.chang image{
  width: 210rpx;
  height: 210rpx;
  display: block;
  margin: auto;
}
.chang .tit{
  width: 100%;
  font-size: 48rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.chang .btn{
  width: 100%;
  height: 80rpx;
  background: linear-gradient(224deg, #11E299 0%, #08C466 100%);
  border-radius: 20rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  display: flex;
  align-items: center;
  justify-content: center;
}
.chang .bark{
  width: 100%;
  height: 80rpx;
  background: linear-gradient(180deg, #FFDEDE 0%, #FCC6C6 100%);
  border-radius: 20rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #2D540E;
  display: flex;
  align-items: center;
  justify-content: center;
}
.chang .ain{
  width: 100%;
  height: 80rpx;
  background: linear-gradient(360deg, #F23232 0%, #F21E1E 55%, #FF7979 100%);       
  border-radius: 20rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 30rpx auto;
