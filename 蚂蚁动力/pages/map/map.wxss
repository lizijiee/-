/* pages/map/map.wxss */
.zuobiao{
  position: absolute;
  top:50%;
  left:50%;
  transform:translate(-50%,-50%)
}
.zuobiao .image{
  width: 56rpx;
  height: 60rpx;
}
.seach{
  width: 90%;
  height: 60rpx;
  margin: 50rpx auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-radius: 20rpx;
  border:1rpx solid #eee;
  background: #fff;
  overflow: hidden;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
  z-index: 9999;
}
.seach input{
  display: block;
  width: 75%;
  height: 100%;
  padding-left: 15rpx;
  font-size: 28rpx;
}
.seach button{
  width: 20%;
  height: 100%;
  color: #fff;
  background: linear-gradient(360deg, #F23232 0%, #F21E1E 55%, #FF7979 100%);
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.btn{
  width: 80%;
  height: 68rpx;
  margin: auto;
  background: linear-gradient(360deg, #F23232 0%, #F21E1E 55%, #FF7979 100%);
  border-radius: 20rpx;
  margin-top: 100rpx;
  color: #fff;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  position: fixed;
  bottom: 100rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 999;
}
.sac{
  position: absolute;
  top: 114rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 90%;
  z-index: 9999;
  background: #fff;
  border-radius: 15rpx;
}
.sac view{
  width: 100%;
  height: 60rpx;
  font-size: 26rpx;
  color: #666;
  padding-left:15rpx;
  display: flex;
  align-items: center;
}