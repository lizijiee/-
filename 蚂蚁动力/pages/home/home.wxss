}

.place {
  font-size: 19rpx;
  color: #333333;
  margin-bottom: 9rpx;
}

.time {
  font-size: 26rpx;
  color: #666;
  /* height: 70rpx; */
  display: flex;
  margin-bottom: 10rpx;
}

.time text {
  font-size: 20rpx;
  color: #999999;
}

.time image {
  height: 20rpx;
