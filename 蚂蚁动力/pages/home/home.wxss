@import '../addcourse/addcourse.wxss';

/* pages/home/home.wxss */
.kong {
  width: 100%;
  height: 150rpx;
}

.vh {
  width: 100%;
  height: 90vh;
}

.woap {
  width: 100vw;
  min-height: calc(100vh - 11vh);
  padding-bottom: 50rpx 0;
  background-color: #FAFAFA;
  margin: 0;
}

.joinlesson {
  width: 662rpx;
  margin: 0 auto;
  background-color: white;
  padding: 30rpx 20rpx 0 20rpx;
  border-radius: 20rpx;
  position: relative;
  border: none;
}

.joinlesson .line {
  /* position: absolute; */
  width: 100%;
  margin: 0;
  /* left: 0; */
  /* top: 63rpx; */
  /* border: 1px solid #F2F2F2; */
}

.joinlesson>view icon {
  height: 32rpx;
  width: 6rpx;
  background-color: #F96041;
  vertical-align: middle;
  margin-right: 17rpx;
}

.joinlesson .title {
  font-size: 30rpx;
  font-weight: bold;
  padding-right: 15rpx;
  vertical-align: middle;
}

.joinlesson .more {
  float: right;
  font-size: 19rpx;
  color: #999999;
}

.joinlesson .more image {
  width: 20rpx;
  height: 20rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

.usercommit {
  width: 100%;
  padding: 0 0 30rpx 0;
}

.usercommit>view {
  padding: 30rpx 20rpx 31rpx 20rpx;
  width: 662rpx;
  margin: 0 auto;
}

.usercommit swiper {
  height: 460rpx;
}

.optionBox {
  height: 460rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.optionBox image {
  height: 380rpx;
  line-height: 460rpx;
}

.activeItem {
  margin: 0;
  width: 434rpx;
  height: 460rpx !important;
  line-height: 460rpx;
}

.activeItem image {
  width: 434rpx;
  height: 460rpx;
  line-height: 460rpx;
  transition-duration: 0.3s;
}

.swiperText {
  width: 100%;
  height: 25px;
  text-align: center;
  font-family: bariol_bold;
  font-size: 0.5em;
  color: #4a4a4a;
}

.mask {
  width: 100%;
  height: 100%;
  background-color: rgba(255, 255, 255, .5);
  position: absolute;
  left: 0;
  top: 0;
}

.perch {
  width: 100%;
  height: 100%;
}

.banner {
  height: 300rpx;
  width: 702rpx;
  margin: 0 auto;
  border-radius: 12rpx;
}

.banner swiper-item {
  height: 100%;
  width: 100%;
  border-radius: 15rpx;
  overflow: hidden;
}

.banner .slide-image {
  height: 100%;
  width: 100%;
}



.culum {
  width: 95%;
  margin: auto;
  background: #fff;
  border-radius: 30rpx;
  margin-bottom: 30rpx;
  box-shadow: 0 2rpx 12rpx 0 rgba（0，0，0，0.1）;
}

.culum .title {
  width: 92%;
  margin: auto;
  height: 100rpx;
  display: flex;
  align-items: center;
}

.culum .title text {
  font-size: 36rpx;
  font-weight: bold;
  padding-right: 15rpx;
}

.culum .title image {
  width: 28rpx;
  height: 24rpx;
}

.eves {
  width: 92%;
  margin: auto;
}

.eve {
  border-bottom: 1px solid #ebedf0;
  width: 100%;
  margin: auto;
  padding-bottom: 30rpx;
  padding-top: 20rpx;
}

/* .eve image {
  width: 100%;
  height: 270rpx;
  border-radius: 30rpx;
} */

.baer {
  width: 100%;
  position: relative;
}

.baer text {
  display: inline-block;
  position: absolute;
  bottom: 30rpx;
  left: 30rpx;
  font-size: 34rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #FFFFFF;
  line-height: 40rpx;
  letter-spacing: 6rpx;
  text-shadow: 5rpx 5rpx 5rpx #000000;
}

.eve .desc {
  width: 90%;
  font-size: 28rpx;
  color: #666;
  margin: 15rpx auto;
  word-break: break-all;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}



.head {
  /* padding: 0 0 168rpx 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  background-color: #FE5838;
  border-radius: 0 0 32rpx 32rpx;
  position: relative;
  margin-bottom: 260rpx; */
  width: 95%;
  margin: 15rpx auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.head .headimg {
  width: 75rpx;
  height: 75rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 0;
}

.head .name {
  width: 100rpx;
  text-align: center;
  /* margin-top: 18rpx; */
}

.head .name text {
  display: block;
}

.head .name text:nth-child(1) {
  width: 100rpx;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.head .name text:nth-child(2) {
  font-size: 22rpx;
}

.head .seach {
  width: 400rpx;
  height: 50rpx;
  border-radius: 100rpx;
  border: 1rpx solid #979797;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
  margin: 0;
}

.head .line {
  width: 1rpx;
  background-color: grey;
  height: 42rpx;
  margin: 0px 5rpx 0 15rpx;
}

.head .seach image {
  width: 24rpx;
  height: 24rpx;
  margin-right: 30rpx;
  margin-left: 0;
}

.head .seach input {
  width: 70%;
  font-size: 24rpx;
}

.head .int {
  width: 110rpx;
  text-align: center;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  justify-content: center;
  color: #333333;
}

.head .int image {
  width: 48rpx;
  height: 48rpx;
  margin: auto;
}

.head .int text {
  font-size: 20rpx;
  font-weight: 400;
  color: #0C0C0C;
}

.selectlesson {
  width: 638rpx;
  position: absolute;
  left: 24rpx;
  bottom: -228rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
  flex-flow: row wrap;
  background-color: white;
  border-radius: 25rpx;
  padding: 0 32rpx 29rpx 32rpx;
}

.selectlesson .joinbutton {
  text-align: center;
  /* flex: 0 0 25%; */
  margin-right: 82rpx;
  margin-top: 30rpx;
}

.selectlesson .joinbutton:nth-child(4),
.selectlesson .joinbutton:nth-child(8) {
  margin-right: 0rpx;
}

.selectlesson .joinbutton image {
  display: block;
  width: 98rpx;
  height: 98rpx;
  margin: 0 auto;

  /* display: block; */
}

.selectlesson .joinbutton text {
  color: #333333;
  font-size: 24rpx;
  font-weight: 600;
}

.navs {
  width: 100%;
  height: 160rpx;
  line-height: 190rpx;
  background: #FE5838;
  position: sticky;
  top: 0;
  z-index: 9;
  color: white;
  font-size: 31rpx;
  font-weight: bold;
  text-align: center;
  vertical-align: middle;
}

.ation {
  color: #fff;
  position: absolute;
  bottom: 32rpx;
  left: 20rpx;
  font-size: 30rpx;
  display: flex;
  align-items: center;
}

.ation text {
  display: block;
  max-width: 360rpx;
  overflow: hidden;
  /*超出部分隐藏*/
  white-space: nowrap;
  /*不换行*/
  text-overflow: ellipsis;
  /*超出部分文字以...显示*/
}

.ation image {
  width: 30rpx;
  height: 15rpx;
  padding-left: 20rpx;
}



@keyframes scale {
  from {
    -webkit-transform: scale(1);
    transform: scale(1);
  }

  to {
    -webkit-transform: scale(0.9);
    transform: scale(0.9);
  }
}

.newuser {
  width: 100%;
  margin: auto;
  /* animation: scale 2s  infinite; */
  animation: scale 0.9s ease-in 0s infinite alternate;
  background: white;
}

.newuser>image:nth-child(1) {
  width: 702rpx;
  height: 211rpx;
  display: block;
  margin: 0 auto;
  /* animation: width 2s both infinite; */
}

.newuser>image:nth-child(2) {
  width: 100%;
  height: 25rpx;
  display: block;
  /* animation: width 2s both infinite; */
}

.newuser .notice {
  width: 702rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 0;
  padding-top: 17rpx;
  padding-bottom: 30rpx;
  margin: 0 auto;
}

.newuser .noticeitem image {
  width: 20rpx;
  height: 20rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}

.newuser .noticeitem text {
  font-size: 18rpx;
  vertical-align: middle;
}

.display {
  width: 92%;
  margin: 40rpx auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.display image {
  width: 32%;
  height: 54rpx;
}

.lists {
  position: absolute;
  top: 80rpx;
  left: 68rpx;
  width: 78%;
  background: #fff;
  max-height: 240rpx;
  z-index: 9999;
}

.lists text {
  display: block;
  width: 100%;
  height: 60rpx;
  font-size: 26rpx;
  color: #666;
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #eee;
  padding-left: 15rpx;
}

.mains {
  width: 500rpx;
  height: 400rpx;
  overflow: hidden;
}

.mains view {
  display: flex;
  align-items: center;
  width: 96%;
  height: 70rpx;
  color: #666;
  font-size: 26rpx;
  margin: 10rpx auto;
}

@keyframes width {
  0% {
    width: 100%;
    height: 100%;
  }

  50% {
    width: 90%;
    height: 90%;
  }

  100% {
    width: 100%;
    height: 100%;
  }
}

.joinlessonlist {
  /* padding-top: 30rpx; */
  height: 810rpx;
  overflow: auto;
}


.right {
  width: 60%;
  padding: 20rpx 0;
}

.ae {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.left image {
  width: 228rpx;
  height: 180rpx;
  border-radius: 15rpx;
}

.top {
  width: 100%;
  height: inherit;
  font-size: 30rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
